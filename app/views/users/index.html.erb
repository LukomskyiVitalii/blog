<h1>Users</h1>
<ul>
  <% @users.each do |user| %>
    <li>
      <%= link_to user.name, user %>
      <%= link_to "Destroy", user_path(user),
                  method: :delete,
                  data: { confirm: "Are you sure?" } %>
    </li>
  <% end %>
</ul>
<br>
 <%= link_to "Cancel", root_path %>